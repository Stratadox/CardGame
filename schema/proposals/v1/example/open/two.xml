<?xml version="1.0"?>
<proposals:resource
    rel="self"
    href="test://match/proposals/open/(ACCOUNT)/"
    xmlns:account="https://github.com/Stratadox/CardGame/tree/master/docs/relation/account/"
    xmlns:proposals="https://github.com/Stratadox/CardGame/tree/master/docs/resource/proposals/collection/"
    xmlns:proposal="https://github.com/Stratadox/CardGame/tree/master/docs/relation/proposals/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/Stratadox/CardGame/tree/master/docs/resource/proposals/collection/  /schema/proposals/v1/collection.xsd"
>
    <link rel="account:overview" href="test://account/(ACCOUNT)/" />
    <proposals:resource rel="proposal:overview" href="test://match/proposals/(PROPOSAL)/">
        <link rel="proposal:from" href="test://account/(OTHER-ACCOUNT)/" />
        <link rel="proposal:to" href="test://account/(ACCOUNT)/" />
        <link rel="proposal:accept" href="test://match/proposals/{PROPOSAL}/accept/" />
        <id>(PROPOSAL)</id>
        <from>(OTHER-ACCOUNT)</from>
        <to>(ACCOUNT)</to>
        <valid-until>(EXPIRE)</valid-until>
    </proposals:resource>
    <proposals:resource rel="proposal:overview" href="test://match/proposals/(PROPOSAL2)/">
        <link rel="proposal:from" href="test://account/(THIRD-ACCOUNT)/" />
        <link rel="proposal:to" href="test://account/(ACCOUNT)/" />
        <link rel="proposal:accept" href="test://match/proposals/(PROPOSAL2)/accept/" />
        <id>(PROPOSAL2)</id>
        <from>(THIRD-ACCOUNT)</from>
        <to>(ACCOUNT)</to>
        <valid-until>(EXPIRE)</valid-until>
    </proposals:resource>
</proposals:resource>
